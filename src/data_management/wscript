#! python


def build(ctx):

    # Illustrate simple use of run_py_script
    ctx(
        features='run_py_script',
        source='data_generation.py',
        target=ctx.path_to(ctx, 'OUT_DATA', 'y.csv'),
	
        name1='y'
	
    )
    ctx(
        features='run_py_script',
        source='data_generation.py',
        
	target=ctx.path_to(ctx, 'OUT_DATA', 'x.csv'),
	
	
        
	name='X'
	
	
    )
    ctx(
        features='run_py_script',
        source='data_generation.py',
        
	target=ctx.path_to(ctx, 'OUT_DATA', 'beta.csv'),
	
        
	name='beta'
	
    )
    ctx(
        features='run_py_script',
        source='data_generation.py',
        
	target=ctx.path_to(ctx, 'OUT_DATA', 'eps.csv'),
        
	name='eps'
    )
